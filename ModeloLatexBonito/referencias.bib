journal = {Procedia Computer Science},
keywords = {Optical character recognition (OCR),Optical font recognition (OFR),Scale invariant feature transform (SIFT)},
mendeley-groups = {Optical Font Recognition},
pages = {1055--1059},
publisher = {Elsevier},
title = {{Farsi/Arabic optical font recognition using SIFT features}},
url = {http://dx.doi.org/10.1016/j.procs.2010.12.173},
