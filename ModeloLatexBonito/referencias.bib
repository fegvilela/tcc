keywords = {a disability condition caused,accessibility,by uncorrectable,disability,legibility,low vision,millions of people have,of the eye,or partially correctable disorders,rehabilitation is increasing access,the primary goal of,this paper describes how,to printed material,typography},
mendeley-groups = {Deficientes Visuais etc},
number = {5},
pages = {469--482},
pmid = {15204299},
title = {{Adjustable typography: an approach to enhancing low vision text accessibility.}},
volume = {47},
year = {2004}
}


